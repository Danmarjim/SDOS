<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dani.prueba.sdos"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="20" />

    <application
        android:name="com.dani.prueba.sdos.SDOSApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher2"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.dani.prueba.sdos.ui.activities.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.dani.prueba.sdos.ui.activities.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.dani.prueba.sdos.ui.activities.AdministradorActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>